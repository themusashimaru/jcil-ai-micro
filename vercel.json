{
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "crons": [
    {
      "path": "/api/devotional/daily",
      "schedule": "1 0 * * *"
    },
    {
      "path": "/api/cron/process-pending",
      "schedule": "* * * * *"
    }
  ]
}
