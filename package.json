    "octokit": "^5.0.5",
    "openai": "^6.16.0",
    "opossum": "^9.0.0",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.4",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.7.0",
    "stripe": "^20.1.2",
